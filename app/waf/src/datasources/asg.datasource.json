{
  "connector": "rest",
  "options": {
    "headers": {
      "accept": "application/json",
      "content-type": "application/json"
    },
    "strictSSL": false
  },
  "operations": [
    {
      "template": {
        "method": "PUT",
        "url": "https://{host}:{port}/mgmt/shared/TrustedDevices",
        "body": "{body}"
      },
      "functions": {
        "trust": [
          "host",
          "port",
          "body"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "url": "https://{host}:{port}/mgmt/shared/TrustedDevices/{deviceId}"
      },
      "functions": {
        "queryTrust": [
          "host",
          "port",
          "deviceId"
        ]
      }
    },
    {
      "template": {
        "method": "DELETE",
        "url": "https://{host}:{port}/mgmt/shared/TrustedDevices/{deviceId}"
      },
      "functions": {
        "untrust": [
          "host",
          "port",
          "deviceId"
        ]
      }
    }
  ]
}
