{
  "connector": "rest",
  "options": {
    "headers": {
      "accept": "application/json",
      "content-type": "application/json"
    },
    "strictSSL": false
  },
  "operations": [
    {
      "template": {
        "method": "PUT",
        "headers": "{headers}",
        "url": "https://{host}:{port}/mgmt/shared/TrustedDevices",
        "body": "{body}"
      },
      "functions": {
        "trust": [
          "host",
          "port",
          "body",
          "headers"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "headers": "{headers}",
        "url": "https://{host}:{port}/mgmt/shared/TrustedDevices/{deviceId}"
      },
      "functions": {
        "queryTrust": [
          "host",
          "port",
          "deviceId",
          "headers"
        ]
      }
    },
    {
      "template": {
        "method": "DELETE",
        "headers": "{headers}",
        "url": "https://{host}:{port}/mgmt/shared/TrustedDevices/{deviceId}"
      },
      "functions": {
        "untrust": [
          "host",
          "port",
          "deviceId",
          "headers"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "headers": "{headers}",
        "url": "https://{host}:{port}/mgmt/shared/TrustedExtensions/{deviceId}"
      },
      "functions": {
        "queryExtensions": [
          "host",
          "port",
          "deviceId",
          "headers"
        ]
      }
    },
    {
      "template": {
        "method": "POST",
        "headers": "{headers}",
        "url": "https://{host}:{port}/mgmt/shared/TrustedASMPolicies",
        "body": "{body}"
      },
      "functions": {
        "uploadWafpolicyByUrl": [
          "host",
          "port",
          "body",
          "headers"
        ]
      }
    },
    {
      "template": {
        "method": "POST",
        "headers": "{headers}",
        "url": "https://{host}:{port}/mgmt/shared/TrustedExtensions/{deviceId}",
        "body": "{body}"
      },
      "functions": {
        "install": [
          "host",
          "port",
          "deviceId",
          "body",
          "headers"
        ]
      }
    },
    {
      "template": {
        "method": "POST",
        "headers": "{headers}",
        "url": "{url}",
        "body": "{body}"
      },
      "functions": {
        "deploy": [
          "url",
          "body",
          "headers"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "headers": "{headers}",
        "url": "https://{host}:{port}/mgmt/shared/TrustedASMPolicies/{trustDeviceId}?policyName={wafpolicyName}"
      },
      "functions": {
        "checkWafpolicyByName": [
          "host",
          "port",
          "trustDeviceId",
          "wafpolicyName",
          "headers"
        ]
      }
    }
  ]
}
